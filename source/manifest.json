{
	"name": "TT plugged in",
	"version": "0.0.0",
	"description": "TT plugged in provides autobopping, and volume control among others",
	"homepage_url": "https://github.com/nthitz/pluggedTT",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"storage"
	],
	"background": {
		"persistent": false,
		"scripts": [
			"webext-options-sync.js",
			"options-storage.js", 
			"background.js"
		]
	},
	"content_scripts": [
		{
			"matches": ["*://*.turntable.fm/*"],
			"css": ["myStyles.css"],
			"js": [
				"webext-options-sync.js",
				"options-storage.js", 
				"contentScript.js"
			],
			"all_frames": true
		}
	],
	"web_accessible_resources": [
		"modal.html",
		"localContextScript.js.txt"
	]
}
